<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link href="https://fonts.googleapis.com/css2?family=IBM+Plex+Mono:ital@0;1&family=Roboto:wght@400;500;700&display=swap" rel="stylesheet"> 
  <link rel="stylesheet" href="/style.css">
  <title>Home Page</title>
</head>
<body>
  <header class="header">
    <a href="#">CarRental</a>
    <nav>
      <ul>
        <li><a href="#">Home</a></li>
        <li><a href="#">Contato</a></li>
        <li><a href="#">Veiculos</a></li>
      </ul>
    </nav>
  </header>
  <section class="intro">

  </section>
  <section class="cars">
    <h1>Cars</h1>
    <ul>
      <% cars.forEach(car => { %>
        <li>
          <div>
            <h2><%= car.name %></h2>
            <p>Modelo: <span><%= car.model %></span></p>
            <p>Ano: <span><%= /\d{4}/.exec(car.year) %></span></p>
          </div>
        </li>
      <% }) %>
    </ul>
  </section>
</body>
</html>